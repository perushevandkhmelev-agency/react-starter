{
  "presets": ["react", "env"],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "transform-class-properties",
    "add-module-exports",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "styles": "./styles",
          "translate": "./intl"
        }
      }
    ],
    [
      "react-intl",
      {
        "messagesDir": "./intl/src"
      }
    ]
  ]
}
