{
  "presets": ["react", "env"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-object-rest-spread",
    "transform-class-properties",
    "add-module-exports",
    [
      "module-resolver",
      {
        "root": ["./"]
      }
    ],
    [
      "react-intl",
      {
        "messagesDir": "./intl/",
        "enforceDescriptions": true
      }
    ]
  ]
}
