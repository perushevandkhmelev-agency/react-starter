{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["react"],
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "rules": {
    "semi": [2, "never"],
    "strict": [2, "global"],
    "eol-last": [0],
    "quotes": [2, "single"],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "key-spacing": [0],
  }
}
